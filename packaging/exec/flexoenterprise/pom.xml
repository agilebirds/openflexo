<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<name>Flexo Enterprise Packaging</name>
	<parent>
		<groupId>org.openflexo.packaging</groupId>
		<artifactId>exec</artifactId>
		<version>1.4.4-SNAPSHOT</version>
	</parent>
	<artifactId>flexoenterprise</artifactId>
    <packaging>pom</packaging>
	<build>
        <plugins>
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>exec-maven-plugin</artifactId>
                <version>1.2</version>
                <executions>
                    <execution>
                        <goals>
                            <goal>exec</goal>
                        </goals>
                    </execution>
                </executions>
                <configuration>
                    <classpathScope>runtime</classpathScope>
                    <executable>java</executable>

                    <arguments>
                        <argument>-Xverify:all</argument>
                        <argument>-Xmx1G</argument>
                        <argument>-Xdock:name=Flexo</argument>
                        <argument>-Djava.util.logging.config.file=Config/logging.properties</argument>
                        <argument>-Dapple.laf.useScreenMenuBar=true</argument>

                        <argument>-classpath</argument>
                        <classpath/>

                        <argument>org.openflexo.Flexo</argument>
                        <argument>-userType</argument>
                        <argument>developer_release</argument>

                    </arguments>

                </configuration>
            </plugin>
        </plugins>
    </build>
