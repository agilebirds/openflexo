<?xml version="1.0" encoding="UTF-8"?>
<Panel className="org.openflexo.fib.editor.controller.FIBInspector" layout="border" darkLevel="0" dataClassName="org.openflexo.fib.model.FIBButton" name="Inspector">
  <TabPanel name="Tab" constraints="border(location=center)">
    <Tab title="Basic" layout="twocols" titleFont="SansSerif,0,11" darkLevel="0" index="0" font="SansSerif,0,11" useScrollBar="true" horizontalScrollbarPolicy="HORIZONTAL_SCROLLBAR_AS_NEEDED" verticalScrollbarPolicy="VERTICAL_SCROLLBAR_AS_NEEDED" name="BasicTab">
      <Label label="label" constraints="twocols(location=left)" localize="true" />
      <TextField data="data.label" constraints="twocols(expandHorizontally=true;location=right)" localize="true" columns="15" />
      <Label label="button_icon" constraints="twocols(location=left)" localize="true" />
      <Custom componentClassName="org.openflexo.fib.utils.BindingSelector" localize="true" manageDynamicModel="true" data="data.buttonIcon.binding" constraints="twocols(expandHorizontally=true;location=right)">
        <Assignment variable="component.bindable" value="data" />
        <Assignment variable="component.bindingDefinition" value="data.BUTTON_ICON" />
      </Custom>
      <Label label="button_type" constraints="twocols(location=left)" localize="true" />
      <DropDown data="data.buttonType" constraints="twocols(location=right)" localize="true" iteratorClassName="org.openflexo.fib.model.FIBButton$ButtonType" autoSelectFirstRow="false" />
      <Label label="is_default" constraints="twocols(expandHorizontally=true;location=left)" localize="true" />
      <CheckBox selected="false" data="data.isDefault" constraints="twocols(location=right)" localize="true" />
    </Tab>
    <Tab title="Controls" layout="twocols" titleFont="SansSerif,0,11" darkLevel="0" index="2" font="SansSerif,0,11" useScrollBar="true" horizontalScrollbarPolicy="HORIZONTAL_SCROLLBAR_AS_NEEDED" verticalScrollbarPolicy="VERTICAL_SCROLLBAR_AS_NEEDED" name="ControlsTab">
      <Label label="action" constraints="twocols(location=left)" localize="true" />
      <Custom componentClassName="org.openflexo.fib.utils.BindingSelector" localize="true" manageDynamicModel="true" data="data.action.binding" constraints="twocols(expandHorizontally=true;location=right)">
        <Assignment variable="component.bindable" value="data" />
        <Assignment variable="component.bindingDefinition" value="data.ACTION" />
      </Custom>
    </Tab>
  </TabPanel>
  <Parameter name="title" value="Button" />
</Panel>
