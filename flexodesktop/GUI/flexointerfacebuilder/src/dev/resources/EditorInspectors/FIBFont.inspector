<Panel className="org.openflexo.fib.editor.controller.FIBInspector" 
	   name="Inspector" 
	   layout="border" 
	   dataClassName="org.openflexo.fib.model.FIBFont">
	<Parameter name="title" value="Font selector"/>

		<TabPanel name="Tab" constraints="border(location=center)">

			<Tab name="BasicTab" title="Basic" index="0" layout="twocols" font="SansSerif,0,11" >
			
				<Label label="sample_text" constraints="twocols(location=left;expandHorizontally=true)"/>
				<TextField data="data.sampleText" constraints="twocols(location=right)" columns="15"/>
				
			</Tab>
						
		</TabPanel>

</Panel>
