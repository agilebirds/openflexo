<Panel className="org.openflexo.fib.editor.controller.FIBInspector" 
	   name="Inspector" 
	   layout="border" 
	   dataClassName="org.openflexo.fib.model.FIBCheckBox">
	<Parameter name="title" value="CheckBox"/>

		<TabPanel name="Tab" constraints="border(location=center)">

			<Tab name="BasicTab" title="Basic" index="0" layout="twocols" font="SansSerif,0,11" >
			
				<CheckBox data="data.negate" constraints="twocols(location=left)" columns="15"/>
				<Label label="negate" constraints="twocols(location=right;expandHorizontally=true)"/>
				
				<CheckBox data="data.selected" constraints="twocols(location=left)" columns="15"/>
				<Label label="selected_by_default" constraints="twocols(location=right;expandHorizontally=true)"/>
				
			</Tab>
						
		</TabPanel>

</Panel>
