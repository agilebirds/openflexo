    <TOCEntry id="SYL_1352" title="Objectives and Scopes" index="1" isReadOnly="false" renderSectionTitle="true" startOnANewPage="false" includeStatusList="true" recursionEnabled="true" content="" lastUpdate="HH:mm:ss dd/MM/yyyy,14:02:08 02/07/2012" userID="SYL" flexoID="1352" docFormat="HTML" dontGenerate="false" useSpecificDescriptions="false">
      <TOCEntry id="SYL_1353" title="Purpose" index="1" isReadOnly="false" renderSectionTitle="true" startOnANewPage="false" includeStatusList="true" recursionEnabled="true" content="&lt;p&gt;&#xA;&lt;span style=&quot;background-color:#ffffcc;&quot;&gt;Describe the purpose of this document.&lt;/span&gt;&#xA;&lt;/p&gt;&#xA;&#xA;&lt;p&gt;&#xA;This document is written for the business users to express and validate their &#xA;need specifications and for the application designer to define the full &#xA;technical requirement specifications that will be given to  developers. This &#xA;document is not supposed to be technical and should be understandable by the &#xA;business.&#xA;&lt;/p&gt;" lastUpdate="HH:mm:ss dd/MM/yyyy,14:02:08 02/07/2012" userID="SYL" flexoID="1353" docFormat="HTML" dontGenerate="false" useSpecificDescriptions="false" />
      <TOCEntry id="SYL_1354" title="Scope" index="2" isReadOnly="false" renderSectionTitle="true" startOnANewPage="false" includeStatusList="true" recursionEnabled="true" content="&lt;p&gt;&#xA;&lt;span style=&quot;background-color:#ffffcc;&quot;&gt;A brief description of the scope of &#xA;this business requirements specification.&#xA;&lt;/span&gt;&lt;/p&gt;&#xA;&#xA;&lt;p&gt;&#xA;&lt;span style=&quot;background-color:#ffffcc;&quot;&gt;&lt;em&gt;In this subsection:&lt;/em&gt;&#xA;&lt;/span&gt;&lt;/p&gt;&#xA;&#xA;&lt;p&gt;&#xA;&lt;span style=&quot;background-color:#ffffcc;&quot;&gt;&lt;em&gt;(1)&amp;#160;&amp;#160; &amp;#160;Identify the &#xA;software product(s) to be produced by name.&lt;/em&gt;&#xA;&lt;/span&gt;&lt;/p&gt;&#xA;&#xA;&lt;p&gt;&#xA;&lt;span style=&quot;background-color:#ffffcc;&quot;&gt;&lt;em&gt;(2)&amp;#160; &amp;#160;Explain what the &#xA;software product(s) will, and, if necessary, will not do.&lt;/em&gt;&#xA;&lt;/span&gt;&lt;/p&gt;&#xA;&#xA;&lt;p&gt;&#xA;&lt;span style=&quot;background-color:#ffffcc;&quot;&gt;&lt;em&gt;(3)&amp;#160; &amp;#160;Describe the &#xA;application of the software being specified, including relevant benefits, &#xA;objectives, and goals .&lt;/em&gt;&#xA;&lt;/span&gt;&lt;/p&gt;&#xA;&#xA;&lt;p&gt;&#xA;&lt;span style=&quot;background-color:#ffffcc;&quot;&gt;&lt;em&gt;(4)&amp;#160; &amp;#160;Include a short &#xA;description of how it works with other systems (if appropriate). &lt;/em&gt;&#xA;&lt;/span&gt;&lt;/p&gt;&#xA;&#xA;&lt;p&gt;&#xA;&lt;span style=&quot;background-color:#ffffcc;&quot;&gt;&lt;em&gt;(5)&amp;#160; &amp;#160;Be consistent with &#xA;similar statements in higher-level specifications if they exist&lt;/em&gt;&#xA;&lt;/span&gt;&lt;/p&gt;&#xA;&#xA;&lt;p&gt;&#xA;&lt;span style=&quot;background-color:#ffffcc;&quot;&gt;&lt;em&gt;&amp;#160;&lt;/em&gt;&#xA;&lt;/span&gt;&lt;/p&gt;&#xA;&#xA;&lt;p&gt;&#xA;&lt;span style=&quot;background-color:#ffffcc;&quot;&gt;&lt;em&gt;This should be an executive-level &#xA;summary.&amp;#160; Do not enumerate the whole requirements list here.&lt;/em&gt;&#xA;&lt;/span&gt;&lt;/p&gt;" lastUpdate="HH:mm:ss dd/MM/yyyy,14:02:08 02/07/2012" userID="SYL" flexoID="1354" docFormat="HTML" dontGenerate="false" useSpecificDescriptions="false" />
      <TOCEntry id="SYL_1355" title="Document Overview" index="3" isReadOnly="false" renderSectionTitle="true" startOnANewPage="false" includeStatusList="true" recursionEnabled="true" content="&lt;p&gt;&#xA;&lt;span style=&quot;background-color:#ffffcc;&quot;&gt;Summarize the purpose and contents of &#xA;this document.&#xA;&lt;/span&gt;&lt;/p&gt;" lastUpdate="HH:mm:ss dd/MM/yyyy,14:02:08 02/07/2012" userID="SYL" flexoID="1355" docFormat="HTML" dontGenerate="false" useSpecificDescriptions="false" />
      <TOCEntry id="SYL_1356" title="References" index="4" isReadOnly="false" renderSectionTitle="true" startOnANewPage="false" includeStatusList="true" recursionEnabled="true" content="&lt;span style=&quot;background-color:#ffffcc;&quot;&gt;Identify sources of information used &#xA;to develop this document, such as the Functional Specification, IEEE or &#xA;project documentation.&lt;/span&gt;" lastUpdate="HH:mm:ss dd/MM/yyyy,14:02:08 02/07/2012" userID="SYL" flexoID="1356" docFormat="HTML" dontGenerate="false" useSpecificDescriptions="false" />
      <TOCEntry id="SYL_1357" title="Outstanding issues" index="5" isReadOnly="false" renderSectionTitle="true" startOnANewPage="false" includeStatusList="true" recursionEnabled="true" content="&lt;p&gt;&#xA;&lt;span style=&quot;background-color:#ffffcc;&quot;&gt;A list of issues or problems which are &#xA;known to be outstanding with this revision.&lt;/span&gt;&#xA;&lt;/p&gt;" lastUpdate="HH:mm:ss dd/MM/yyyy,14:02:08 02/07/2012" userID="SYL" flexoID="1357" docFormat="HTML" dontGenerate="false" useSpecificDescriptions="false" />
