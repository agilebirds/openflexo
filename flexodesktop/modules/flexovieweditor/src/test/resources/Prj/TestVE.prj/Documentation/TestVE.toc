        <TOCEntry id="SYL_1382" title="Component or system description" index="1" isReadOnly="false" renderSectionTitle="true" startOnANewPage="false" includeStatusList="true" recursionEnabled="true" content="&lt;span style=&quot;background-color:#ffffcc;&quot;&gt;A brief introduction to the component &#xA;or system that is covered by thee business specification. This section should &#xA;be brief, since it is included only to help the reader quickly understand what &#xA;is being specified. A context diagram should be included to assist in &#xA;positioning the proposed component or system.&lt;/span&gt;" lastUpdate="HH:mm:ss dd/MM/yyyy,14:02:08 02/07/2012" userID="SYL" flexoID="1382" docFormat="HTML" dontGenerate="false" useSpecificDescriptions="false" />
