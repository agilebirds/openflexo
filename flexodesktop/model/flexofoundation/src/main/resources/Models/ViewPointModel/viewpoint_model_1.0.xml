<?xml version="1.0" encoding="UTF-8"?>

<model builder="org.openflexo.foundation.viewpoint.ViewPoint$ViewPointBuilder"  handlesReferences="yes" serializationMode="pseudoTree">

  <entity name="org.openflexo.foundation.viewpoint.ViewPointObject" abstract="YES" finalizer="finalizeDeserialization">
   <description>Root class for all objects of ViewPoint model</description>
  </entity>

	<entity name="org.openflexo.foundation.viewpoint.NamedViewPointObject" abstract="yes">
		<property name="name" xmlTag="name" attribute="YES" />
		<property name="description" xmlTag="description" />
	</entity>

	<entity name="org.openflexo.foundation.viewpoint.ViewPoint"
		xmlTag="ViewPoint" finalizer="finalizeDeserialization">
		<property name="viewPointURI" xmlTag="uri" attribute="YES" />
		<property name="version" xmlTag="version" attribute="YES" />
		<property name="modelVersion" xmlTag="modelVersion" attribute="YES" />
		<property name="localizedDictionary" xmlTag="LocalizedDictionary" />
		<property name="modelSlots" type="vector" contains="org.openflexo.foundation.technologyadapter.ModelSlot"/>
	</entity>

	<entity name="org.openflexo.foundation.technologyadapter.ModelSlot" abstract="yes" contexts="Addressed">
		<property name="isRequired" xmlTag="isRequired" attribute="YES" />
		<property name="isReadOnly" xmlTag="isReadOnly" attribute="YES" />
		<property name="metaModelURI" xmlTag="metaModelURI" attribute="YES" />
	</entity>

	<entity name="org.openflexo.technologyadapter.emf.EMFModelSlot" xmlTag="EMFModelSlot">
	</entity>

	<entity name="org.openflexo.technologyadapter.owl.OWLModelSlot" xmlTag="OWLModelSlot">
	</entity>

	<entity name="org.openflexo.technologyadapter.xsd.XSDModelSlot" xmlTag="XSDModelSlot">
	</entity>

	<entity name="org.openflexo.foundation.viewpoint.LocalizedDictionary"
		xmlTag="LocalizedDictionary">
		<property name="entries" type="vector" xmlTag="Localized" />
	</entity>

	<entity name="org.openflexo.foundation.viewpoint.LocalizedEntry"
		xmlTag="Localized">
		<property name="key" xmlTag="key" attribute="YES" />
		<property name="language" xmlTag="lang" attribute="YES" />
		<property name="value" text="yes" />
	</entity>

</model>
