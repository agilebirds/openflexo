  <EditionPattern id="100" name="Employee">
    <description>Allows to define concept of employee in a context of basic organization tree</description>
    <DropScheme id="101" target="http://www.agilebirds.com/openflexo/ontologies/OrganizationTree/BasicOrganizationTree.owl#OrganizationCell" name="DropEmployeeInOrganization" skipConfirmationPanel="false">
      <Parameter id="102" name="roleName" label="role_name" widget="TEXT_FIELD" defaultValue="enter role name here" usePaletteLabelAsDefaultValue="true" uniqueURI="false">
        <description>Allow to enter role name while dropping concept</description>
      </Parameter>
      <Parameter id="103" name="employeeURI" label="employee_uri" widget="TEXT_FIELD" defaultValue="" usePaletteLabelAsDefaultValue="false" uniqueURI="true" base="roleName">
        <description>Allow to enter (unique) individual name used in Ontology</description>
      </Parameter>
      <Parameter id="104" name="isManager" label="is_manager" widget="CHECKBOX" defaultValue="false" usePaletteLabelAsDefaultValue="false" uniqueURI="false">
        <description>When set to true, indicates that created employee concept will address a manager</description>
      </Parameter>
      <Parameter id="105" name="isHeadOfOrganization" label="is_head_of_organization" widget="CHECKBOX" defaultValue="false" usePaletteLabelAsDefaultValue="false" uniqueURI="false" conditional="isManager=true">
        <description>When set to true, indicates that created employee is at the head of current organization cell</description>
      </Parameter>
      <Parameter id="106" name="assignPerson" label="assign_employee" widget="CHECKBOX" defaultValue="false" usePaletteLabelAsDefaultValue="false" uniqueURI="false">
        <description>When set to true, indicates that created employee concept will be assigned to a given person</description>
      </Parameter>
      <Parameter id="107" name="employeeFirstName" label="employee_first_name" widget="TEXT_FIELD" defaultValue="" usePaletteLabelAsDefaultValue="false" uniqueURI="false" conditional="assignPerson=true">
        <description>Employee first name</description>
      </Parameter>
      <Parameter id="108" name="employeeLastName" label="employee_last_name" widget="TEXT_FIELD" defaultValue="" usePaletteLabelAsDefaultValue="false" uniqueURI="false" conditional="assignPerson=true">
        <description>Employee last name</description>
      </Parameter>
      <Parameter id="109" name="personURI" label="person_uri" widget="TEXT_FIELD" defaultValue="" usePaletteLabelAsDefaultValue="false" uniqueURI="true" base="employeeFirstName+employeeLastName" conditional="assignPerson=true">
        <description>Allow to enter (unique) individual name used in Ontology</description>
      </Parameter>
      <AddIndividual id="110" individualName="employeeURI" patternRole="employeeAsManager" conditional="isManager=true" conceptURI="http://www.agilebirds.com/openflexo/ViewPoints/Tests/BasicOrganizationTreeEditor.owl#BOTManager">
