        <TOCEntry id="FLX_33" title="Component or system description" index="1" isReadOnly="false" renderSectionTitle="true" startOnANewPage="false" includeStatusList="true" recursionEnabled="true" content="&lt;span style=&quot;background-color:#ffffcc;&quot;&gt;A brief introduction to the component &#10;or system that is covered by thee business specification. This section should &#10;be brief, since it is included only to help the reader quickly understand what &#10;is being specified. A context diagram should be included to assist in &#10;positioning the proposed component or system.&lt;/span&gt;" lastUpdate="HH:mm:ss dd/MM/yyyy,22:08:21 08/02/2013" userID="FLX" flexoID="33" docFormat="HTML" dontGenerate="false" useSpecificDescriptions="false"/>
