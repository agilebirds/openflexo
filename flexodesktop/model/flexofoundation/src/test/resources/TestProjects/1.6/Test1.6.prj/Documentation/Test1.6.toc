    <TOCEntry id="FLX_3" title="Objectives and Scopes" index="1" isReadOnly="false" renderSectionTitle="true" startOnANewPage="false" includeStatusList="true" recursionEnabled="true" content="" lastUpdate="HH:mm:ss dd/MM/yyyy,22:08:21 08/02/2013" userID="FLX" flexoID="3" docFormat="HTML" dontGenerate="false" useSpecificDescriptions="false">
      <TOCEntry id="FLX_4" title="Purpose" index="1" isReadOnly="false" renderSectionTitle="true" startOnANewPage="false" includeStatusList="true" recursionEnabled="true" content="&lt;p&gt;&#10;&lt;span style=&quot;background-color:#ffffcc;&quot;&gt;Describe the purpose of this document.&lt;/span&gt;&#10;&lt;/p&gt;&#10;&#10;&lt;p&gt;&#10;This document is written for the business users to express and validate their &#10;need specifications and for the application designer to define the full &#10;technical requirement specifications that will be given to  developers. This &#10;document is not supposed to be technical and should be understandable by the &#10;business.&#10;&lt;/p&gt;" lastUpdate="HH:mm:ss dd/MM/yyyy,22:08:21 08/02/2013" userID="FLX" flexoID="4" docFormat="HTML" dontGenerate="false" useSpecificDescriptions="false"/>
      <TOCEntry id="FLX_5" title="Scope" index="2" isReadOnly="false" renderSectionTitle="true" startOnANewPage="false" includeStatusList="true" recursionEnabled="true" content="&lt;p&gt;&#10;&lt;span style=&quot;background-color:#ffffcc;&quot;&gt;A brief description of the scope of &#10;this business requirements specification.&#10;&lt;/span&gt;&lt;/p&gt;&#10;&#10;&lt;p&gt;&#10;&lt;span style=&quot;background-color:#ffffcc;&quot;&gt;&lt;em&gt;In this subsection:&lt;/em&gt;&#10;&lt;/span&gt;&lt;/p&gt;&#10;&#10;&lt;p&gt;&#10;&lt;span style=&quot;background-color:#ffffcc;&quot;&gt;&lt;em&gt;(1)&amp;#160;&amp;#160; &amp;#160;Identify the &#10;software product(s) to be produced by name.&lt;/em&gt;&#10;&lt;/span&gt;&lt;/p&gt;&#10;&#10;&lt;p&gt;&#10;&lt;span style=&quot;background-color:#ffffcc;&quot;&gt;&lt;em&gt;(2)&amp;#160; &amp;#160;Explain what the &#10;software product(s) will, and, if necessary, will not do.&lt;/em&gt;&#10;&lt;/span&gt;&lt;/p&gt;&#10;&#10;&lt;p&gt;&#10;&lt;span style=&quot;background-color:#ffffcc;&quot;&gt;&lt;em&gt;(3)&amp;#160; &amp;#160;Describe the &#10;application of the software being specified, including relevant benefits, &#10;objectives, and goals .&lt;/em&gt;&#10;&lt;/span&gt;&lt;/p&gt;&#10;&#10;&lt;p&gt;&#10;&lt;span style=&quot;background-color:#ffffcc;&quot;&gt;&lt;em&gt;(4)&amp;#160; &amp;#160;Include a short &#10;description of how it works with other systems (if appropriate). &lt;/em&gt;&#10;&lt;/span&gt;&lt;/p&gt;&#10;&#10;&lt;p&gt;&#10;&lt;span style=&quot;background-color:#ffffcc;&quot;&gt;&lt;em&gt;(5)&amp;#160; &amp;#160;Be consistent with &#10;similar statements in higher-level specifications if they exist&lt;/em&gt;&#10;&lt;/span&gt;&lt;/p&gt;&#10;&#10;&lt;p&gt;&#10;&lt;span style=&quot;background-color:#ffffcc;&quot;&gt;&lt;em&gt;&amp;#160;&lt;/em&gt;&#10;&lt;/span&gt;&lt;/p&gt;&#10;&#10;&lt;p&gt;&#10;&lt;span style=&quot;background-color:#ffffcc;&quot;&gt;&lt;em&gt;This should be an executive-level &#10;summary.&amp;#160; Do not enumerate the whole requirements list here.&lt;/em&gt;&#10;&lt;/span&gt;&lt;/p&gt;" lastUpdate="HH:mm:ss dd/MM/yyyy,22:08:21 08/02/2013" userID="FLX" flexoID="5" docFormat="HTML" dontGenerate="false" useSpecificDescriptions="false"/>
      <TOCEntry id="FLX_6" title="Document Overview" index="3" isReadOnly="false" renderSectionTitle="true" startOnANewPage="false" includeStatusList="true" recursionEnabled="true" content="&lt;p&gt;&#10;&lt;span style=&quot;background-color:#ffffcc;&quot;&gt;Summarize the purpose and contents of &#10;this document.&#10;&lt;/span&gt;&lt;/p&gt;" lastUpdate="HH:mm:ss dd/MM/yyyy,22:08:21 08/02/2013" userID="FLX" flexoID="6" docFormat="HTML" dontGenerate="false" useSpecificDescriptions="false"/>
      <TOCEntry id="FLX_7" title="References" index="4" isReadOnly="false" renderSectionTitle="true" startOnANewPage="false" includeStatusList="true" recursionEnabled="true" content="&lt;span style=&quot;background-color:#ffffcc;&quot;&gt;Identify sources of information used &#10;to develop this document, such as the Functional Specification, IEEE or &#10;project documentation.&lt;/span&gt;" lastUpdate="HH:mm:ss dd/MM/yyyy,22:08:21 08/02/2013" userID="FLX" flexoID="7" docFormat="HTML" dontGenerate="false" useSpecificDescriptions="false"/>
      <TOCEntry id="FLX_8" title="Outstanding issues" index="5" isReadOnly="false" renderSectionTitle="true" startOnANewPage="false" includeStatusList="true" recursionEnabled="true" content="&lt;p&gt;&#10;&lt;span style=&quot;background-color:#ffffcc;&quot;&gt;A list of issues or problems which are &#10;known to be outstanding with this revision.&lt;/span&gt;&#10;&lt;/p&gt;" lastUpdate="HH:mm:ss dd/MM/yyyy,22:08:21 08/02/2013" userID="FLX" flexoID="8" docFormat="HTML" dontGenerate="false" useSpecificDescriptions="false"/>
