#macro(renderSpecificDescription $_object $usedTarget)
<!--Entering macro renderSpecificDescription -->
    <w:sdt>
        <w:sdtPr>
            #if($usedTarget)
                <w:alias w:val="$usedTarget description"/>
                #if(!${_object.hasSpecificDescriptionForKey($usedTarget)})
