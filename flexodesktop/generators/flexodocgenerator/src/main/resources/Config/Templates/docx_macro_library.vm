#macro(tocEntry $_entry)
<!--Entering macro tocEntry -->
    $!{generator.beginTocEntry($_entry)}
    #if(${_entry.getStartOnANewPage()})
        <w:p>
            <w:r>
                <w:br w:type="page"/>
            </w:r>
        </w:p>
    #end
    ##set($templateName = "#getTemplateNameForTocEntry($_entry)")
    ##set($templateName = "#getTemplateNameForTocEntry($_entry)")
    #set($templateName = ${_entry.getDefaultTemplateName()})
    
    #if(${_entry.isIteration()})
    	${generator.log("Rendering iteration")}

		#foreach($_item in ${generator.getIterableObjects($_entry)})
			${generator.iterateOnItem($_entry,$_item)}
			#if($generator.getItemCondition($_entry))
    			${generator.log("Item is valid, performing rendering for this iteration")}
    			#subTocEntries($_entry)
			#end
		#end
    
