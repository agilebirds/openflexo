    #renderTocEntryContent($_entry)
    #if($templateName)
        #set($newEntryLevel = $_entry.getLevel() + 1)
        #set($passedObject = $generator.getAccessedModelObject($_entry)))
    	${generator.log($templateName, $passedObject)}
        #render($passedObject $newEntryLevel $templateName $_entry)
        ##render($generator.getCondition($_entry) $newEntryLevel $templateName $_entry)
    #end
    #subTocEntries($_entry)
    #end
    ${generator.endTocEntry($_entry)}

<!-- Exiting macro tocEntry -->
#end

